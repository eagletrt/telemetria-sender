    "Nicola Toscan <nicolatoscan99@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/mongodb": "^3.5.32",
    "@types/node": "^14.14.6",
    "@types/shelljs": "^0.8.8",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@eagletrt/eagletrt-telemetria-simulator": "0.0.2",
    "chai": "^4.2.0",
    "dree": "^2.5.0",
    "mocha": "^8.2.1",
    "mongo-cleaner": "^3.3.0",
    "mongodb": "^3.6.2",
    "shelljs": "^0.8.4"
  }
